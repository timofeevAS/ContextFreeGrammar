<Sentence> ::= <NounGroup><Whitespace><VerbGroup>`.`|<Question><Whitespace><VerbGroup>`?`

<Question> ::= <Hs><Whitespace><QuestionNoun>|<Hv><Whitespace><PronounWithHave>

<QuestionNoun> ::= <Noun>|<Name>

<NounGroup> ::= <NounH>|<PronounH>

<NounH> ::= <Adjective><Whitespace><Noun><Whitespace><Hs>|<Noun><Whitespace><Hs>

<PronounH> ::= <PronounWithHas><Whitespace><Hs>|<PronounWithHave><Whitespace><Hv>

<Hs> ::= `has`|`hasn't`

<Hv> ::= `have`|`haven't`

<PronounWithHas> ::= `it`|`she`|`he`

<PronounWithHave> ::= `i`|`you`|`we`|`they`

<Noun> ::= <Name>|<PeopleWord>

<Name> ::= `Alexey`|`Mikhail`|`Vadim`|`Dmitry`|`Sergey`|`Vladimir`|`Alexandr`

<PeopleWord> ::= `man`|`woman`|`boy`|`girl`|`dad`|`mom`|`brother`|`sister`

<Adjective> ::= `cool`|`strong`|`clever`|`dark`|`large`

<VerbGroup> ::= <MarkerBeforeVerb><Whitespace><Verb>|<Verb><SpecialPostfix>

<SpecialPostfix> ::= <Empty>|<Whitespace><MarkerInTheEnd>

<MarkerBeforeVerb> ::= `ever`|`never`|`just`|`already`

<Verb> ::= `run`|`learnt`<Whitespace><CourseName>|`did`<Whitespace><Action>|`felt`<Whitespace><Feeling>

<CourseName> ::= `math`|`informatics`|`programming`|`physics`

<Action> ::= `repairs`|`hometask`|`dinner`|`writes`

<Feeling> ::= `excellent`|`tired`|`happy`|`proud`

<Empty> ::= ``

<Whitespace> ::= ` `

<MarkerInTheEnd> ::= `yet`|`recently`|`lately`|`before`
